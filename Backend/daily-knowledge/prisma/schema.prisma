generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String  @id
  name     String
  username String  @unique
  password String
  admin    Boolean @default(false)
}

model Post {
  id      String @id
  title   String @unique
  text    String
  example String
  author  String
  url     String
}

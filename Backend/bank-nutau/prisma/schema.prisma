generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         String  @id
  name       String
  last_name  String
  nickname   String  @unique
  email      String  @unique
  password   String
  cpf        String  @unique
  account_id String
  admin      Boolean @default(false)
}
